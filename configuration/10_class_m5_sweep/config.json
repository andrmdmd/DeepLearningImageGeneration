  "training": {
    "engine": "sweep_engine",
    "batch_size": 64,
    "num_workers": 4,
    "epochs": 50,
    "lr": 0.001,
    "weight_decay": 0.0001
  },
  "log_dir": "logs",
  "project_dir": "m5_sweep",
  "project_tracker": [
    "wandb"
  ],
  "evaluation": {
    "batch_size": 32,
    "num_workers": 4
  },
  "model": {
    "architecture": "M5"
  },
  "sweep": {
    "config": "configuration/10_class_m5_sweep/sweep_config.json",
    "project_name": "m5_sweep"
  }
}