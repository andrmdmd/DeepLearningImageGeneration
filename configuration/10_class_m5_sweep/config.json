{
  "data": {
    "root": "data",
    "representation": "waveform",
    "sample_rate": 16000,
    "yes_no_binary": false,
    "unknown_commands_included": false,
    "target_commands": [
      "yes",
      "no",
      "up",
      "down",
      "left",
      "right",
      "on",
      "off",
      "stop",
      "go"
    ],
    "background_noise": false,
    "n_fft": 512,
    "hop_length": 202,
    "n_mels": 80,
    "n_mfcc": 80
